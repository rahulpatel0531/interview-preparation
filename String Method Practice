let str = "JavaScript";

console.log(str.length);
console.log(str[2]);
console.log("charAt", str.charAt(2));
console.log("charCodeAt", str.charCodeAt(2));
console.log("indexOf", str.indexOf('S'));
console.log("toLowerCase", str.toLowerCase());
console.log("toUpperCase", str.toUpperCase());
console.log("slice", str.slice(2, 5))
console.log("substring", str.substring(2, 5))
console.log("concat", str.concat(" Dev"))
console.log("split", "JavaScript Developer".split(' '))
console.log("includes", "JavaScript dev Developer".includes('dev'))
console.log("replace", "JavaScript Developer".replace('JavaScript', 'NodeJS'))
console.log("Java Dev JS Dev NodeJS dev".replaceAll('Dev', 'Developer'))
console.log("replaceAll", "Java Dev JS Dev NodeJS dev".replaceAll('Dev', 'Developer')) 
console.log("trim",   "JavaScript Developer  ".trim())
// console.log("   JavaScript Developer  ".trimStart())
// console.log("   JavaScript Developer  ".trimEnd())
console.log("padStart","Dev".padStart(4, '*'))
console.log("padEnd", "Dev".padEnd(4, '*'));
console.log("startsWith", "JavaScript Developer".startsWith("Java"))
// console.log("JavaScript Developer".endsWith("per"))
console.log("repeat", "JavaScript Developer".repeat(3))
console.log("indexOf", "JavaScript Developer To Developer".indexOf('Developer'))
console.log("lastIndexOf", "JavaScript Developer To Developer".lastIndexOf('Developer'))
console.log("search", "JavaScript Developer".search('vel'))